  "bit-DP": {
    "description": "bit DP",
    "prefix": "dp by subgroups",
    "body": [
      "//bit DP",
      "vector<vector<ll>> dp(1 << N, vector<ll>(N, INF));",
      "dp[0][0] = 0;",
      "rep(i, 1 << N)",
      "{",
      "  rep(j, N)",
      "  {",
      "    if (dp[i][j] == INF)",
      "      continue;",
      "    rep(k, N)",
      "    {",
      "      // 隣接行列表現です。",
      "      if (G[j][k] == INF)",
      "        continue;",
      "      if (i & (1 << k))",
      "        continue;",
      "      chmin(dp[i | (1 << k)][k], dp[i][j] + G[j][k]);",
      "    }",
      "  }",
      "}",
      "ll res = LLONG_MAX;",
      "chmin(res, dp[(1 << N) - 1][0]);",
      "if (res == LLONG_MAX)",
      "  cout << -1 << endl;",
      "else",
      "  cout << res << endl;"
